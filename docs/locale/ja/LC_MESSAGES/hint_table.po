msgid "Parameter name"
msgstr "パラメータ名"

#: ../../hint_table.md 82105f6e9ad84cf9a57efd18de5c588a
msgid "Description"
msgstr "説明"

#: ../../hint_table.md 1505ffc204664aea82ee49df94e5db36
msgid "Default"
msgstr "デフォルト値"

#: ../../hint_table.md 7e428113623648d1a71cf79670ea1d0d
